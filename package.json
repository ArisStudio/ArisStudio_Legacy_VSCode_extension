{
  "name": "baplayer-command",
  "icon": "images/icon.png",
  "displayName": "ArisStudio command",
  "description": "A extension for ArisStudio.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tualin14/baplayer-command"
  },
  "homepage": "https://github.com/Tualin14/baplayer-command",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "publisher": "dzaaaaaa",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ArisStudio",
        "aliases": [
          "ArisStudio",
          "ArisStudio"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ArisStudio",
        "scopeName": "source.txt",
        "path": "./syntaxes/as.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ArisStudio",
        "path": "./snippets/as.code-snippets"
      }
    ]
  }
}